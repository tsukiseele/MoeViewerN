          },
          "originUrl": {
            "selector": "$(ul#post-list-posts > li > a[href]).attr(href)"
          },
          "coverUrl": {
            "selector": "$(div > a.thumb > img).attr(src)"
          }
        }
      ]
    },
    "search": {
      "index": "https://lolibooru.moe/post?tags={keywords:}&page={page:}",
      "reuse": "home"
    }
  }
