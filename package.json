{
  "name": "moe-viewer-n",
  "author": "TsukiSeele",
  "description": "a booru browser",
  "license": "GPL-3.0",
  "version": "0.0.0",
  "scripts": {
    "build": "vite build",
    "build:for:electron": "cross-env ELECTRON=true vite build",
    "dev": "vite",
    "electron": "wait-on tcp:3000 && cross-env IS_DEV=true electron .",
    "electron:pack": "electron-builder --dir",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none yarn dev\" \"yarn electron\"",
    "electron:build": "yarn build:for:electron && electron-builder",
    "preview": "vite preview --port 5050"
  },
  "main": "./main/esm-loader.js",
  "build": {
    "appId": "com.tsukiseele.moeviewern",
    "productName": "MoeViewerN",
    "copyright": "Copyright Â© 2022 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "dist/**/*",
      "main/**/*"
    ],
    "directories": {
      "buildResources": "public",
      "output": "dist_electron"
    },
    "extraFiles": {
      "from": "./static/",
      "to": "static"
    }
  },
  "dependencies": {
    "@mdi/font": "~6.6.96",
    "abortcontroller-polyfill": "~1.7.3",
    "cheerio": "~1.0.0-rc.10",
    "delay": "^5.0.0",
    "electron-log": "~4.4.6",
    "esm": "^3.2.25",
    "get-proxy-settings": "~0.1.13",
    "https-proxy-agent": "~5.0.0",
    "js-base64": "^3.7.2",
    "lodash": "^4.17.21",
    "naive-ui": "^2.27.0",
    "node-fetch": "~2.6.7",
    "p-limit": "~4.0.0",
    "p-timeout": "~5.0.2",
    "pinia": "~2.0.11",
    "vue": "~3.2.31",
    "vue-router": "~4.0.12"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "~2.2.2",
    "@vitejs/plugin-vue-jsx": "~1.3.7",
    "concurrently": "~7.0.0",
    "cross-env": "~7.0.3",
    "electron": "~17.3.1",
    "electron-builder": "~22.14.13",
    "pug": "~3.0.2",
    "sass": "~1.49.9",
    "vite": "~2.8.4",
    "wait-on": "~6.0.1"
  }
}
